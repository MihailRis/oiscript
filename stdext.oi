# Call function with all items as argument sequentally
func apply(arr, fn):
    for i : arr:
        fn(i)

# Return random integer in range [min, max] inclusive
func randint(min, max):
    return int(rand() * (max - min + 1)) + min

# Returns true with probability defined as t / f
func randbool(t, f):
    return rand() <= t / (t + f)